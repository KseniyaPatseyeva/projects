@model Parking.Models.Car
@{
    ViewBag.Title = "Удаление машины";
}

<div>
    <dl class="dl-horizontal">
        <dt>Номер машины</dt>
        <dd>
            @Html.DisplayFor(model => model.carNumber)
        </dd>

        <dt>Присутствие</dt>
        <dd>
            @Html.DisplayFor(model => model.isIn)
        </dd>
    </dl>

    <div>
        <form asp-controller="Home" asp-action="Delete" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Удалить" />
            </div>
        </form>
    </div>
</div>


